{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: cl.exe build active file",
            "command": "cl.exe",
            "args": [
                "/Zi",
                "/EHsc",
                "/I${fileDirname}/CSFML-2.5-windows-64-bit/include",
                "${fileDirname}/CSFML-2.5-windows-64-bit/lib/msvc/csfml-graphics.lib",
                "${fileDirname}/CSFML-2.5-windows-64-bit/lib/msvc/csfml-window.lib",
                "${fileDirname}/CSFML-2.5-windows-64-bit/lib/msvc/csfml-system.lib",
                "${file}",
                "/link",
                "/MACHINE:x64"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "problemMatcher": [
                "$msCompile"
            ],
            "group": {
                "kind": "test",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}